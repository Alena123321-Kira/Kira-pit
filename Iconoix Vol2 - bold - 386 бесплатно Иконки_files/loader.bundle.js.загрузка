ay")})),this.addDestroyHandler((()=>{this.playingState.dispatch("end")})),this.onBeforeComplete.add((()=>{this.trackVastEvent("beforeComplete",{})})),this.addDestroyHandler((e=>{let{reason:t}=e;"skip"!==t&&"close"!==t||this.trackVastEvent(t,{})})),this.vpaidCreativeAdStartedSignal.add((()=>{this.creativeAdStartedCallback()})),this.addDestroyHandler((()=>{this.onResume.removeAll(),this.onPause.removeAll(),this.playingState.removeAll(),this.playbackPosition.removeAll(),this.onFirstQuartile.removeAll(),this.onMidpoint.removeAll(),this.onThirdQuartile.removeAll(),this.onBeforeComplete.removeAll(),this.onComplete.removeAll(),this.onVolumeChange.removeAll(),this.adVideoIsBuffering.removeAll(),this.vpaidCreativeAdStartedSignal.removeAll(),this.vpaidCreativeAdErrorSignal.removeAll()})),this.addDestroyHandler(Ne({element:window,eventName:"unload",listener:()=>{this.destroy({reason:"unload",details:void 0})}})),this.isStarted.addOne(this.subscribeFullscreenForLogs);const h=new Error,{vpaidTimeout:p}=n;this.shouldPlayTimeCounter.addCallback(p,(()=>{const e=this.getScriptSrc(),t=this.lifeCycleStage.getValue(),{videoNodeController:i}=this.adDisplayController,{videoNode:o}=i,r={adPodId:this.mediaAd.getAdPodId(),timeout:p,src:e,config:this.adDisplayController.playbackParameters,viewerPlaybackParams:n,liveCycleStage:t,videoNodeSrc:o.src,adMediaState:this.adDisplayController.adMediaState.getValue(),networkState:o.networkState,readyState:o.readyState,downlink:this.adDisplayController.getAdPlacementOptions().downlink};this.isDestroyed?s.error({error:new Lu({code:"SHOULD_PLAY_TIMER_IS_STARTED_AFTER_VIEWER_DESTROY",details:r})}):(this.isAdPlayingCalled&&!this.isStarted.getValue()&&s.error({error:new Lu({code:"VPAID_TIMEOUT_PREVENTED_BY_AD_PLAYING_EVENT",details:r})}),this.isStarted.getValue()||this.isAdPlayingCalled||this.destroy({reason:"error",details:new Lu(h,{code:gs({creativeLifeCycleStage:t,isYandexCreative:nh(this.getScriptSrc()),adCreativeWrapper:this.adCreativeWrapper}),details